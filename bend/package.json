{
  "name": "tracelinkexplainerbend",
  "version": "1.0.0",
  "description": "API for accessing data to explain traces between artifacts.",
  "main": "src/server.js",
  "scripts": {
    "killOld": "lsof -t -i tcp:5000 | xargs kill",
    "start": "'ts-node' src/server.ts",
    "start-watch": "npm run killOld & nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec npm start",
    "lint": "eslint . --ext .ts,.tsx,.js",
    "test": "jest --forceExit --maxWorkers=1 --detectOpenHandles",
    "test-watch": "jest --watch --forceExit --maxWorkers=1 --detectOpenHandles"
  },
  "author": "Alberto Rodriguez",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/eslint": "^7.2.0",
    "@types/express": "^4.17.6",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "python-shell": "^2.0.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.14",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "eslint": "^7.3.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}