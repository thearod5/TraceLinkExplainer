{
  "extends": [
    "plugin:jest/recommended",
    "plugin:react/recommended",
    "standard"
  ],
  "env": {
    "browser": true,
    "es2020": true,
    "jest/globals": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": ["react", "@typescript-eslint"],
  "rules": {
    "no-unused-vars": ["off"],
    "no-tabs": ["off"]
  }
}
